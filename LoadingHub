-- =========================================================
--  xSOLITOx HUB - SCRIPT COMPLETO v1.0
--  CONTIENE: Librería Recreada + Definición Completa de la UI
--  - Código 100% nuevo, limpio y funcional.
--  - Apariencia visual 1:1 con el original + modificaciones.
--  - Borde brilloso, nombres y colores actualizados.
--  - Lógica de funciones vacía, lista para adaptar.
-- =========================================================

-- PARTE 1: LA LIBRERÍA COMPLETA (RECREADA DESDE CERO)

local Library = {}
Library.__index = Library

do
	-- Limpieza inicial para evitar conflictos
	if game:GetService("CoreGui"):FindFirstChild("xSOLITOxHUB_GUI") then
		game:GetService("CoreGui").xSOLITOxHUB_GUI:Destroy()
	end
	if game:GetService("CoreGui"):FindFirstChild("xSOLITOxHUB_Icon") then
		game:GetService("CoreGui").xSOLITOxHUB_Icon:Destroy()
	end

	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")

	local Styles = {
		Colors = {
			Primary = Color3.fromRGB(1, 94, 255),
			Dark = Color3.fromRGB(22, 22, 26),
			LighterDark = Color3.fromRGB(35, 35, 40),
			Text = Color3.fromRGB(255, 255, 255),
			SubText = Color3.fromRGB(150, 150, 150),
			GradientStart = Color3.fromRGB(1, 94, 255),
			GradientEnd = Color3.fromRGB(255, 5, 70)
		},
		Fonts = {
			Title = Enum.Font.GothamBold,
			Subtitle = Enum.Font.Cartoon,
			Tab = Enum.Font.Roboto,
			Header = Enum.Font.GothamBold,
			Text = Enum.Font.Cartoon,
			Desc = Enum.Font.Gotham,
			Dropdown = Enum.Font.Nunito
		}
	}

	local function ApplyDefaultGradient(element)
		if element:FindFirstChildOfClass("UIGradient") then return end
		local gradient = Instance.new("UIGradient")
		gradient.Color = ColorSequence.new({
			ColorSequenceKeypoint.new(0, Styles.Colors.GradientStart),
			ColorSequenceKeypoint.new(1, Styles.Colors.GradientEnd)
		})
		gradient.Rotation = 45
		gradient.Parent = element
	end

	function Library.new(config)
		local self = setmetatable({}, Library)

		self.ScreenGui = Instance.new("ScreenGui")
		self.ScreenGui.Name = "xSOLITOxHUB_GUI"
		self.ScreenGui.Parent = game:GetService("CoreGui")
		self.ScreenGui.DisplayOrder = 999
		self.ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

		local OutlineMain = Instance.new("Frame")
		OutlineMain.Name = "OutlineMain"
		OutlineMain.Parent = self.ScreenGui
		OutlineMain.AnchorPoint = Vector2.new(0.5, 0.5)
		OutlineMain.Position = UDim2.new(0.5, 0, 0.45, 0)
		OutlineMain.Size = UDim2.new(0, config.Size.X.Offset, 0, config.Size.Y.Offset)
		OutlineMain.BackgroundColor3 = Styles.Colors.Dark
		OutlineMain.BackgroundTransparency = 0.1
		OutlineMain.ClipsDescendants = false
		local corner = Instance.new("UICorner", OutlineMain)
		corner.CornerRadius = UDim.new(0, 15)

		local borderStroke = Instance.new("UIStroke")
		borderStroke.Thickness = 10
		borderStroke.LineJoinMode = Enum.LineJoinMode.Round
		borderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
		borderStroke.Parent = OutlineMain

		local colorGradient = Instance.new("UIGradient")
		colorGradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Styles.Colors.GradientStart), ColorSequenceKeypoint.new(1, Styles.Colors.GradientEnd)})
		colorGradient.Rotation = 45
		colorGradient.Parent = borderStroke

		local transparencyGradient = Instance.new("UIGradient")
		transparencyGradient.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0, 0.2), NumberSequenceKeypoint.new(0.5, 0.8), NumberSequenceKeypoint.new(1, 1)})
		transparencyGradient.Rotation = 120
		transparencyGradient.Parent = borderStroke

		local Main = Instance.new("Frame")
		Main.Name = "Main"
		Main.Parent = OutlineMain
		Main.AnchorPoint = Vector2.new(0.5, 0.5)
		Main.Position = UDim2.new(0.5, 0, 0.5, 0)
		Main.Size = UDim2.new(1, -20, 1, -20)
		Main.BackgroundTransparency = 1
		Main.ClipsDescendants = true
		local mainCorner = Instance.new("UICorner", Main)
		mainCorner.CornerRadius = UDim.new(0, 12)

		local TopBar = Instance.new("Frame")
		TopBar.Name = "TopBar"
		TopBar.Parent = Main
		TopBar.Size = UDim2.new(1, 0, 0, 40)
		TopBar.BackgroundTransparency = 1

		local Title = Instance.new("TextLabel")
		Title.Name = "Title"
		Title.Parent = TopBar
		Title.Font = Styles.Fonts.Title
		Title.Text = config.Title or "xSOLITOx HUB"
		Title.TextSize = 20
		Title.TextColor3 = Styles.Colors.Text
		Title.BackgroundTransparency = 1
		Title.TextXAlignment = Enum.TextXAlignment.Left
		Title.Position = UDim2.new(0, 15, 0.5, 0)
		Title.AnchorPoint = Vector2.new(0, 0.5)
		Title.Size = UDim2.fromOffset(game:GetService("TextService"):GetTextSize(Title.Text, Title.TextSize, Title.Font, Vector2.new()).X, 25)

		local SubTitle = Instance.new("TextLabel")
		SubTitle.Name = "SubTitle"
		SubTitle.Parent = TopBar
		SubTitle.Font = Styles.Fonts.Subtitle
		SubTitle.Text = config.SubTitle
		SubTitle.TextSize = 15
		SubTitle.TextColor3 = Styles.Colors.SubText
		SubTitle.BackgroundTransparency = 1
		SubTitle.TextXAlignment = Enum.TextXAlignment.Left
		SubTitle.AnchorPoint = Vector2.new(0, 0.5)
		SubTitle.Position = UDim2.new(0, Title.AbsoluteSize.X + 25, 0.5, 0)
		SubTitle.Size = UDim2.fromOffset(game:GetService("TextService"):GetTextSize(SubTitle.Text, SubTitle.TextSize, SubTitle.Font, Vector2.new()).X, 25)

		local CloseButton = Instance.new("ImageButton")
		CloseButton.Name = "CloseButton"
		CloseButton.Parent = TopBar
		CloseButton.Size = UDim2.new(0, 20, 0, 20)
		CloseButton.Position = UDim2.new(1, -15, 0.5, 0)
		CloseButton.AnchorPoint = Vector2.new(1, 0.5)
		CloseButton.BackgroundTransparency = 1
		CloseButton.Image = "rbxassetid://7743878857"
		CloseButton.MouseButton1Click:Connect(function() self.ScreenGui.Enabled = false end)

		local TabContainer = Instance.new("ScrollingFrame")
		TabContainer.Name = "TabContainer"
		TabContainer.Parent = Main
		TabContainer.Size = UDim2.new(0, config.TabWidth, 1, -48)
		TabContainer.Position = UDim2.new(0, 8, 0, 40)
		TabContainer.BackgroundTransparency = 1
		TabContainer.BorderSizePixel = 0
		TabContainer.ScrollBarThickness = 0
		
		local TabListLayout = Instance.new("UIListLayout", TabContainer)
		TabListLayout.Padding = UDim.new(0, 2)
		TabListLayout.SortOrder = Enum.SortOrder.LayoutOrder

		local PageContainer = Instance.new("Frame")
		PageContainer.Name = "PageContainer"
		PageContainer.Parent = Main
		PageContainer.Size = UDim2.new(1, -config.TabWidth - 25, 1, -48)
		PageContainer.Position = UDim2.new(0, config.TabWidth + 18, 0, 40)
		PageContainer.BackgroundTransparency = 1
		PageContainer.ClipsDescendants = true

		local UIPageLayout = Instance.new("UIPageLayout", PageContainer)
		UIPageLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIPageLayout.TweenTime = 0.2

		self.TabContainer = TabContainer
		self.PageContainer = PageContainer
		self.Tabs = {}
		self.CurrentTab = nil

		local function MakeDraggable(obj, target)
			local dragging, dragStart, startPos
			obj.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging, dragStart, startPos = true, input.Position, target.Position
				end
			end)
			UserInputService.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
				end
			end)
			UserInputService.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
					local delta = input.Position - dragStart
					target.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
				end
			end)
		end
		MakeDraggable(TopBar, OutlineMain)

		return self
	end
	
	function Library:Tab(name, icon)
		local TabObject = {}
		
		local TabButton = Instance.new("TextButton", self.TabContainer)
		TabButton.Name, TabButton.Size, TabButton.Text, TabButton.BackgroundTransparency, TabButton.AutoButtonColor = name, UDim2.new(1, 0, 0, 35), "", 1, false
		
		local SelectedIndicator = Instance.new("Frame", TabButton)
		SelectedIndicator.Name = "SelectedTab"
		SelectedIndicator.Size = UDim2.new(0, 3, 0, 0)
		SelectedIndicator.Position = UDim2.new(0, 0, 0.5, 0)
		SelectedIndicator.AnchorPoint = Vector2.new(0, 0.5)
		ApplyDefaultGradient(SelectedIndicator)
		local indCorner = Instance.new("UICorner", SelectedIndicator); indCorner.CornerRadius = UDim.new(1,0)

		local Icon = Instance.new("ImageLabel", TabButton)
		Icon.Name = "Icon"; Icon.Size = UDim2.new(0, 15, 0, 15); Icon.Position = UDim2.new(0, 7, 0.5, 0)
		Icon.AnchorPoint = Vector2.new(0, 0.5); Icon.BackgroundTransparency = 1; Icon.Image = icon; Icon.ImageTransparency = 0.4
		
		local Title = Instance.new("TextLabel", TabButton)
		Title.Name = "Title"; Title.Size = UDim2.new(1, -30, 1, 0); Title.Position = UDim2.new(0, 30, 0, 0)
		Title.BackgroundTransparency = 1; Title.Font = Styles.Fonts.Tab; Title.Text = name; Title.TextSize = 14
		Title.TextColor3 = Styles.Colors.SubText; Title.TextXAlignment = Enum.TextXAlignment.Left

		local Page = Instance.new("ScrollingFrame", self.PageContainer)
		Page.Name = name .. "_Page"; Page.Size = UDim2.new(1, 0, 1, 0); Page.BackgroundTransparency = 1
		Page.BorderSizePixel = 0; Page.ScrollBarThickness = 3; Page.AutomaticCanvasSize = Enum.AutomaticSize.Y; Page.CanvasSize = UDim2.new()
		
		local PageLayout = Instance.new("UIListLayout", Page)
		PageLayout.Padding = UDim.new(0, 3); PageLayout.SortOrder = Enum.SortOrder.LayoutOrder

		TabButton.MouseButton1Click:Connect(function()
			for _, otherTab in pairs(self.Tabs) do
				TweenService:Create(otherTab.Button, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
				TweenService:Create(otherTab.Indicator, TweenInfo.new(0.2), {Size = UDim2.new(0, 3, 0, 0)}):Play()
				TweenService:Create(otherTab.Icon, TweenInfo.new(0.2), {ImageTransparency = 0.4}):Play()
				TweenService:Create(otherTab.Title, TweenInfo.new(0.2), {TextColor3 = Styles.Colors.SubText}):Play()
			end
			TweenService:Create(TabButton, TweenInfo.new(0.2), {BackgroundTransparency = 0.8}):Play()
			TweenService:Create(SelectedIndicator, TweenInfo.new(0.2), {Size = UDim2.new(0, 3, 0, 15)}):Play()
			TweenService:Create(Icon, TweenInfo.new(0.2), {ImageTransparency = 0}):Play()
			TweenService:Create(Title, TweenInfo.new(0.2), {TextColor3 = Styles.Colors.Text}):Play()
			self.PageContainer.UIPageLayout:JumpTo(Page)
			self.CurrentTab = TabObject
		end)
		
		self.Tabs[name] = {Button=TabButton, Indicator=SelectedIndicator, Icon=Icon, Title=Title, Page=Page}
		if self.CurrentTab == nil then TabButton.MouseButton1Click:Fire() end
		
		local TabContent = {}
		
        function TabContent:Button(text,callback)local Button=Instance.new("Frame",Page);Button.Size=UDim2.new(1,0,0,36);Button.BackgroundTransparency=1;local TextLabel=Instance.new("TextLabel",Button);TextLabel.Size=UDim2.new(1,-50,1,0);TextLabel.Position=UDim2.new(0,20,0,0);TextLabel.BackgroundTransparency=1;TextLabel.Font=Styles.Fonts.Text;TextLabel.Text=text;TextLabel.TextSize=15;TextLabel.TextColor3=Styles.Colors.Text;TextLabel.TextXAlignment=Enum.TextXAlignment.Left;local ClickButton=Instance.new("TextButton",Button);ClickButton.Size=UDim2.new(0,25,0,25);ClickButton.Position=UDim2.new(1,-1,0.5,0);ClickButton.AnchorPoint=Vector2.new(1,0.5);ClickButton.Text="";ClickButton.AutoButtonColor=false;local c=Instance.new("UICorner",ClickButton);c.CornerRadius=UDim.new(0,4);ApplyDefaultGradient(ClickButton);ClickButton.MouseButton1Click:Connect(callback or function()end);return Button;end
        function TabContent:Toggle(text,config,desc,callback)local toggled=config or false;local height=desc and 46 or 36;local Button=Instance.new("TextButton",Page);Button.Size=UDim2.new(1,0,0,height);Button.Text="";Button.BackgroundTransparency=0.8;Button.AutoButtonColor=false;local c1=Instance.new("UICorner",Button);c1.CornerRadius=UDim.new(0,5);local Title=Instance.new("TextLabel",Button);Title.Size=UDim2.new(1,0,0,35);Title.Position=UDim2.new(0,15,0.5,desc and-5 or 0);Title.AnchorPoint=Vector2.new(0,0.5);Title.BackgroundTransparency=1;Title.Font=Styles.Fonts.Text;Title.Text=text;Title.TextSize=15;Title.TextColor3=Styles.Colors.Text;Title.TextXAlignment=Enum.TextXAlignment.Left;if desc then local DescLabel=Instance.new("TextLabel",Title);DescLabel.Size=UDim2.new(1,0,0,16);DescLabel.Position=UDim2.new(0,0,0,22);DescLabel.BackgroundTransparency=1;DescLabel.Font=Styles.Fonts.Desc;DescLabel.Text=desc;DescLabel.TextSize=10;DescLabel.TextColor3=Styles.Colors.SubText;DescLabel.TextXAlignment=Enum.TextXAlignment.Left;end;local Switch=Instance.new("TextButton",Button);Switch.Size=UDim2.new(0,35,0,20);Switch.Position=UDim2.new(1,-10,0.5,0);Switch.AnchorPoint=Vector2.new(1,0.5);Switch.Text="";Switch.AutoButtonColor=false;local sC=Instance.new("UICorner",Switch);sC.CornerRadius=UDim.new(0,10);local Circle=Instance.new("Frame",Switch);Circle.Size=UDim2.new(0,14,0,14);Circle.AnchorPoint=Vector2.new(0,0.5);Circle.BackgroundColor3=Styles.Colors.Text;local cC=Instance.new("UICorner",Circle);cC.CornerRadius=UDim.new(0,10);local function update(state)toggled=state;Circle:TweenPosition(UDim2.new(0,toggled and 17 or 3,0.5,0),"Out","Sine",0.2,true);if toggled then ApplyDefaultGradient(Switch);TweenService:Create(Switch,TweenInfo.new(0.2),{BackgroundTransparency=0}):Play()else for _,v in ipairs(Switch:GetChildren())do if v:IsA("UIGradient")then v:Destroy()end end;Switch.BackgroundColor3=Color3.fromRGB(200,200,200);TweenService:Create(Switch,TweenInfo.new(0.2),{BackgroundTransparency=0.8}):Play()end;end;Switch.MouseButton1Click:Connect(function()toggled=not toggled;update(toggled);pcall(callback,toggled)end);update(toggled);return Button;end
        function TabContent:Label(text)local Frame=Instance.new("Frame",Page);Frame.Size=UDim2.new(1,0,0,30);Frame.BackgroundTransparency=1;local Label=Instance.new("TextLabel",Frame);Label.Size=UDim2.new(1,-30,1,0);Label.Position=UDim2.new(0,30,0,0);Label.BackgroundTransparency=1;Label.Font=Styles.Fonts.Dropdown;Label.TextColor3=Styles.Colors.SubText;Label.TextSize=15;Label.Text=text;Label.TextXAlignment=Enum.TextXAlignment.Left;local obj={};function obj:Set(new)Label.Text=new;end;return obj;end
        function TabContent:Seperator(text)local Sep=Instance.new("Frame",Page);Sep.Size=UDim2.new(1,0,0,36);Sep.BackgroundTransparency=1;local Txt=Instance.new("TextLabel",Sep);Txt.Size=UDim2.new(1,0,1,0);Txt.BackgroundTransparency=1;Txt.Font=Styles.Fonts.Header;Txt.Text=text;Txt.TextSize=14;Txt.TextColor3=Styles.Colors.Text;end
        function TabContent:Line()local Line=Instance.new("Frame",Page);Line.Size=UDim2.new(1,0,0,20);Line.BackgroundTransparency=1;local Inner=Instance.new("Frame",Line);Inner.Size=UDim2.new(1,0,0,1);Inner.Position=UDim2.new(0,0,0,10);Inner.BorderSizePixel=0;ApplyDefaultGradient(Inner);end
        function TabContent:Dropdown(text,options,current,callback)local IsDropped=false;local Dropdown=Instance.new("Frame",Page);Dropdown.ClipsDescendants=true;Dropdown.Size=UDim2.new(1,0,0,40);Dropdown.BackgroundTransparency=0.8;local dC=Instance.new("UICorner",Dropdown);dC.CornerRadius=UDim.new(0,5);ApplyDefaultGradient(Dropdown);local Title=Instance.new("TextLabel",Dropdown);Title.Size=UDim2.new(1,0,0,30);Title.Position=UDim2.new(0,15,0,5);Title.BackgroundTransparency=1;Title.Font=Styles.Fonts.Text;Title.Text=text;Title.TextSize=15;Title.TextColor3=Styles.Colors.Text;Title.TextXAlignment=Enum.TextXAlignment.Left;local Selected=Instance.new("TextButton",Dropdown);Selected.Size=UDim2.new(0,100,0,30);Selected.Position=UDim2.new(1,-5,0,5);Selected.AnchorPoint=Vector2.new(1,0);Selected.BackgroundColor3=Styles.Colors.LighterDark;Selected.Font=Styles.Fonts.Dropdown;Selected.Text="   "..(current or"Select");Selected.TextSize=12;Selected.TextColor3=Styles.Colors.Text;Selected.TextXAlignment=Enum.TextXAlignment.Left;local sC=Instance.new("UICorner",Selected);sC.CornerRadius=UDim.new(0,5);local Arrow=Instance.new("ImageLabel",Selected);Arrow.Size=UDim2.new(0,20,0,20);Arrow.Position=UDim2.new(1,-10,0.5,0);Arrow.AnchorPoint=Vector2.new(1,0.5);Arrow.BackgroundTransparency=1;Arrow.Image="rbxassetid://10709790948";local PageHolder=Instance.new("ScrollingFrame",Dropdown);PageHolder.Size=UDim2.new(1,0,0,0);PageHolder.Position=UDim2.new(0,0,0,40);PageHolder.BackgroundColor3=Styles.Colors.LighterDark;PageHolder.BorderSizePixel=0;PageHolder.Visible=false;local phC=Instance.new("UICorner",PageHolder);phC.CornerRadius=UDim.new(0,5);local pl=Instance.new("UIListLayout",PageHolder);pl.Padding=UDim.new(0,1);for i,v in pairs(options)do local Item=Instance.new("TextButton",PageHolder);Item.Size=UDim2.new(1,0,0,30);Item.Text="   "..v;Item.Font=Styles.Fonts.Dropdown;Item.TextSize=13;Item.TextColor3=Styles.Colors.SubText;Item.BackgroundTransparency=1;Item.TextXAlignment=Enum.TextXAlignment.Left;Item.AutoButtonColor=false;Item.MouseEnter:Connect(function()Item.TextColor3=Styles.Colors.Text end);Item.MouseLeave:Connect(function()if Selected.Text~="   "..v then Item.TextColor3=Styles.Colors.SubText end end);Item.MouseButton1Click:Connect(function()IsDropped=false;Dropdown:TweenSize(UDim2.new(1,0,0,40),"Out","Quad",0.2,true);PageHolder.Visible=false;Arrow:TweenRotation(0,"Out","Quad",0.2,true);Selected.Text="   "..v;for _,child in pairs(PageHolder:GetChildren())do if child:IsA("TextButton")then child.TextColor3=Styles.Colors.SubText end end;Item.TextColor3=Styles.Colors.Text;pcall(callback,v)end);if current==v then Item.TextColor3=Styles.Colors.Text end end;Selected.MouseButton1Click:Connect(function()IsDropped=not IsDropped;local goalSize=IsDropped and UDim2.new(1,0,0,145)or UDim2.new(1,0,0,40);local phSize=IsDropped and UDim2.new(1,0,0,100)or UDim2.new(1,0,0,0);local goalRot=IsDropped and 180 or 0;if IsDropped then PageHolder.Visible=true end;Dropdown:TweenSize(goalSize,"Out","Quad",0.2,true);PageHolder:TweenSize(phSize,"Out","Quad",0.2,true);Arrow:TweenRotation(goalRot,"Out","Quad",0.2,true);if not IsDropped then task.wait(0.2);PageHolder.Visible=false end end);return Dropdown;end
        function TabContent:Slider(text, min, max, set, callback)local value=set or min;local Slider=Instance.new("Frame",self.Page);Slider.Size=UDim2.new(1,0,0,35);Slider.BackgroundTransparency=1;local Title=Instance.new("TextLabel",Slider);Title.Size=UDim2.new(1,0,1,0);Title.Position=UDim2.new(0,15,0,0);Title.BackgroundTransparency=1;Title.Font=Styles.Fonts.Text;Title.Text=text;Title.TextSize=15;Title.TextColor3=Styles.Colors.Text;Title.TextXAlignment=Enum.TextXAlignment.Left;local ValueText=Instance.new("TextLabel",Slider);ValueText.Size=UDim2.new(0,50,1,0);ValueText.Position=UDim2.new(1,-115,0,0);ValueText.BackgroundTransparency=1;ValueText.Font=Styles.Fonts.Desc;ValueText.Text=math.floor(value);ValueText.TextSize=12;ValueText.TextColor3=Styles.Colors.SubText;ValueText.TextXAlignment=Enum.TextXAlignment.Right;local Bar=Instance.new("Frame",Slider);Bar.Size=UDim2.new(0,100,0,4);Bar.Position=UDim2.new(1,-10,0.5,0);Bar.AnchorPoint=Vector2.new(1,0.5);Bar.BackgroundColor3=Styles.Colors.LighterDark;local bC=Instance.new("UICorner",Bar);bC.CornerRadius=UDim.new(1,0);local Fill=Instance.new("Frame",Bar);Fill.Size=UDim2.new((value-min)/(max-min),0,1,0);local fC=Instance.new("UICorner",Fill);fC.CornerRadius=UDim.new(1,0);ApplyDefaultGradient(Fill);local Handle=Instance.new("Frame",Bar);Handle.Size=UDim2.new(0,13,0,13);Handle.Position=UDim2.new((value-min)/(max-min),0,0.5,0);Handle.AnchorPoint=Vector2.new(0.5,0.5);Handle.BackgroundColor3=Styles.Colors.Text;local hC=Instance.new("UICorner",Handle);hC.CornerRadius=UDim.new(1,0);local Dragging=false;local function UpdateSlider(input)local percent=math.clamp((input.Position.X-Bar.AbsolutePosition.X)/Bar.AbsoluteSize.X,0,1);value=math.floor(min+(max-min)*percent);Fill.Size=UDim2.new(percent,0,1,0);Handle.Position=UDim2.new(percent,0,0.5,0);ValueText.Text=value;pcall(callback,value)end;Handle.InputBegan:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 then Dragging=true end end);Bar.InputBegan:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 then Dragging=true;UpdateSlider(input)end end);UserInputService.InputEnded:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 then Dragging=false end end);UserInputService.InputChanged:Connect(function(input)if Dragging and input.UserInputType==Enum.UserInputType.MouseMovement then UpdateSlider(input)end end);return Slider;end
        function TabContent:Textbox(text,disappear,callback)local Textbox=Instance.new("Frame",self.Page);Textbox.BackgroundTransparency=0.8;Textbox.Size=UDim2.new(1,0,0,35);local tbC=Instance.new("UICorner",Textbox);tbC.CornerRadius=UDim.new(0,5);local Label=Instance.new("TextLabel",Textbox);Label.BackgroundTransparency=1;Label.Position=UDim2.new(0,15,0.5,0);Label.Size=UDim2.new(1,0,0,35);Label.Font=Styles.Fonts.Dropdown;Label.AnchorPoint=Vector2.new(0,0.5);Label.TextColor3=Styles.Colors.Text;Label.TextSize=15;Label.Text=text;Label.TextXAlignment=Enum.TextXAlignment.Left;local Input=Instance.new("TextBox",Textbox);Input.BackgroundColor3=Color3.fromRGB(200,200,200);Input.BackgroundTransparency=0.8;Input.Position=UDim2.new(1,-5,0.5,0);Input.AnchorPoint=Vector2.new(1,0.5);Input.Size=UDim2.new(0,80,0,25);Input.Font=Enum.Font.Gotham;Input.Text="";Input.TextColor3=Color3.fromRGB(225,225,225);Input.TextSize=11;Input.ClipsDescendants=true;local iC=Instance.new("UICorner",Input);iC.CornerRadius=UDim.new(0,5);Input.FocusLost:Connect(function(enter)if enter then callback(Input.Text);if disappear then Input.Text=""end end end);return Textbox;end

		return TabContent
	end
	
	return self
end

_G.Update = Library
