.insert(NotificationList, { OutlineFrame, Title });
end

local function RemoveOldestNotification()
	if #NotificationList > 0 then
		local removed = table.remove(NotificationList, 1)
		removed[1]:TweenPosition(UDim2.new(0.5, 0, -0.2, 0), "Out", "Quad", 0.4, true, function() removed[1]:Destroy() end)
	end
end
spawn(function() while task.wait(2) do if #NotificationList > 0 then RemoveOldestNotification() end end end)

