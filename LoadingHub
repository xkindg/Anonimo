		ArrowLeft.Size = UDim2.new(0, 15, 0, 15);
		ArrowLeft.Image = "rbxthumb://type=Asset&id=10709768347&w=150&h=150";
		ArrowLeft.Rotation = 180;

		local buttonConn = TextButton.MouseButton1Click:Connect(callback)
		table.insert(_G.conexiones, buttonConn)
