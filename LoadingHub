-- Función de utilidad para crear bordes redondeados
function CreateRounded(Parent, Size)
	local Rounded = Parent:FindFirstChild("Rounded") or Instance.new("UICorner")
	Rounded.Name = "Rounded";
	Rounded.Parent = Parent;
	Rounded.CornerRadius = UDim.new(0, Size);
end;

-- Función de utilidad para hacer un objeto arrastrable
function MakeDraggable(topbarobject, object)
	local Dragging, DragInput, DragStart, StartPosition
	local function Update(input)
		local Delta = input.Position - DragStart;
		local pos = UDim2.new(StartPosition.X.Scale, StartPosition.X.Offset + Delta.X, StartPosition.Y.Scale, StartPosition.Y.Offset + Delta.Y);
		TweenService:Create(object, TweenInfo.new(0.15), { Position = pos }):Play();
	end;
	local inputBegan = topbarobject.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true;
			DragStart = input.Position;
			StartPosition = object.Position;
			local inputChanged = input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					Dragging = false;
				end;
			end);
			table.insert(_G.conexiones, inputChanged)
		end;
	end);
	local inputChanged = topbarobject.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			DragInput = input;
		end;
	end);
	local serviceChanged = UserInputService.InputChanged:Connect(function(input)
		if input == DragInput and Dragging then
			Update(input);
		end;
	end);
	table.insert(_G.conexiones, inputBegan)
	table.insert(_G.conexiones, inputChanged)
	table.insert(_G.conexiones, serviceChanged)
end;

